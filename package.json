{
  "name": "react-polymer-layout",
  "version": "0.2.4",
  "description": "React layout component inspired by Polymer layout attributes",
  "main": "dist/react-polymer-layout.js",
  "scripts": {
    "build": "mkdir -p dist && node_modules/.bin/babel -o dist/react-polymer-layout.js index.jsx",
    "lint": "node_modules/.bin/jsxhint -c ~/.jshintrc *.jsx",
    "test": "cd demo && ../node_modules/.bin/browserify demo.jsx -t babelify -o bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wizawu/react-polymer-layout"
  },
  "keywords": [
    "react",
    "layout",
    "polymer",
    "dialog"
  ],
  "author": "wizawu",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.21",
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "jsxhint": "^0.12.0",
    "react": "^0.13.3"
  }
}
