{
  "name": "mockxhr",
  "version": "1.3.2",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -d -m commonjs --lib 'dom,es2015,es2015.promise' --strictNullChecks --outDir . js/index.ts",
    "watch": "tsc -d -m commonjs --lib 'dom,es2015,es2015.promise' --strictNullChecks --outDir . -w js/index.ts",
    "prepublish": "npm run build && react-beaker publish ."
